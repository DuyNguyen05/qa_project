<%= form_for @user, url: {controller: :static_pages, action: :home}, method: :get do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email, class: "form-control" %>
  <%= f.label :password %>
  <%= f.password_field :password, class: "form-control" %>
  <%= f.submit "Signin", class: "btn btn-primary" %>
<% end %>
